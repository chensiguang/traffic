	public String toJson()
	{
		StringBuffer strBuffer = new StringBuffer();
		strBuffer.append("{'carid':'");
		strBuffer.append(this.getCarid());
		strBuffer.append("','busLicPlate':'");
		strBuffer.append(this.getBusLicPlate());
		strBuffer.append("','driver1':'");
		strBuffer.append(this.getDriver1());
		strBuffer.append("','phone1':'");
		strBuffer.append(this.getPhone1());
		strBuffer.append("','regtime':'");
		strBuffer.append(DateFormateUtil.dateformate("yyyy-MM-dd hh:mm:ss",
				this.getRegtime()));
		strBuffer.append("','regStartTime':'");
		strBuffer.append(DateFormateUtil.dateformate("yyyy-MM-dd hh:mm:ss",
				this.getRegStartTime()));
		strBuffer.append("','regEndTime':'");
		strBuffer.append(DateFormateUtil.dateformate("yyyy-MM-dd hh:mm:ss",
				this.getRegEndTime()));
		strBuffer.append("','busType':'");
		strBuffer.append(this.getBusType());
		strBuffer.append("','comname':'");
		strBuffer.append(this.company.getComname());
		strBuffer.append("','manufacturer':'");
		strBuffer.append(this.getManufacturer());
		strBuffer.append("','factoryType':'");
		strBuffer.append(this.getFactoryType());
		strBuffer.append("','regCode':'");
		strBuffer.append(this.getRegCode());
		strBuffer.append("'}");
		return strBuffer.toString();
	}
