package com.web.videoupload;

import java.io.Serializable;

/**
 * /oauth2/access_token 接口的返回类 例如：{ "access_token": "ACCESS_TOKEN",
 * "expires_in": 1234, "uid":"12341234"}
 * 
 * @author myao
 */
public class UploadUrl implements Serializable
{

	private static final long serialVersionUID = -7350377572165140313L;

	private String itemCode;
	private String token;
	private Integer uploadUrl;

	public String getItemCode()
	{
		return itemCode;
	}

	public void setItemCode(String itemCode)
	{
		this.itemCode = itemCode;
	}

	public String getToken()
	{
		return token;
	}

	public void setToken(String token)
	{
		this.token = token;
	}

	public Integer getUploadUrl()
	{
		return uploadUrl;
	}

	public void setUploadUrl(Integer uploadUrl)
	{
		this.uploadUrl = uploadUrl;
	}

}