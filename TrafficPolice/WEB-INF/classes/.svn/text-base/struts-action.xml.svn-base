<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE struts PUBLIC    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"    "http://struts.apache.org/dtds/struts-2.3.dtd"><struts>        <!-- 后台管理登录 -->	<package name="default" namespace="/" extends="struts-default">		<action name="login" class="com.web.action.AdminAction" method="Authentication">			<result name="success" type="redirect">page/admin/page/Manage.jsp</result>			<result name="error" type="redirect">page/admin/login.jsp</result>		</action>		 <!-- 后台管理退出 -->		<action name="loginout" class="com.web.action.AdminAction" method="Loginout">			<result name="error" type="redirect">page/admin/login.jsp</result>		</action>	</package>		<!-- 后台管理 -->		<package name="com.web.context.admin" namespace="/page/admin/page" extends="struts-default">		<interceptors>			<!--定义认证拦截器 -->			<interceptor name="authentication" class="com.web.filter.AuthenticationInterceptor" />						<!--定义一个包含权限检查的拦截器栈 -->			<interceptor-stack name="securitystack">				<!--配置自定义的拦截器 -->				<interceptor-ref name="authentication" />				<!--配置内建默认拦截器 -->				<interceptor-ref name="defaultStack" />			</interceptor-stack>		</interceptors>		<default-interceptor-ref name="securitystack" />		<!--定义全局Result -->		<global-results>			<result name="login" type="redirect">/page/admin/jump.html</result>		</global-results>		<!-- 网站配置wqg -->		<action name="config_*" class="com.web.action.ConfigAction"			method="{1}">			<result name="default" type="dispatcher">config/ConfigViewList.jsp</result>			<result name="edit" type="dispatcher">config/ConfigEdit.jsp</result>			<result name="view" type="dispatcher">config/ConfigView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>						              <!-- 系统管理开始wqg -->		<!-- 账号管wqg -->		<action name="account_*" class="com.web.action.sysmanage.AccountAction"			method="{1}">			<result name="default" type="dispatcher">account/AccountViewList.jsp</result>			<result name="edit" type="dispatcher">account/AccountEdit.jsp</result>			<result name="editcom" type="dispatcher">account/ComAccountEdit.jsp</result>			<result name="editorg" type="dispatcher">account/OrgAccountEdit.jsp</result>			<result name="editdriver" type="dispatcher">account/DriverAccountEdit.jsp</result>			<result name="view" type="dispatcher">account/AccountView.jsp</result>			<result name="viewpassword" type="dispatcher">account/PasswordView.jsp</result>			<result name="editpassword" type="dispatcher">account/PasswordEdit.jsp</result>			<result name="adddriver" type="dispatcher">account/AddDriver.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>		    		<!--  角色管理 wqg -->		<action name="role_*" class="com.web.action.sysmanage.RoleAction"			method="{1}">			<result name="default" type="dispatcher">role/RoleViewList.jsp</result>			<result name="edit" type="dispatcher">role/RoleEdit.jsp</result>			<result name="view" type="dispatcher">role/RoleView.jsp</result>			<result name="permission" type="dispatcher">role/Permission.jsp</result>			<result name="permissionedit" type="dispatcher">role/PermissionEdit.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>					<!-- 类型管理 wqg -->		<action name="type_*" class="com.web.action.sysmanage.TypeAction"			method="{1}">			<result name="default" type="dispatcher">type/TypeViewList.jsp</result>			<result name="edit" type="dispatcher">type/TypeEdit.jsp</result>			<result name="view" type="dispatcher">type/TypeView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 权限管理 wqg -->				<action name="permission_*" class="com.web.action.sysmanage.PermissionAction"			method="{1}">			<result name="default" type="dispatcher">permission/PermissionViewList.jsp</result>			<result name="edit" type="dispatcher">permission/PermissionEdit.jsp</result>			<result name="view" type="dispatcher">permission/PermissionView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action> 		               <!-- 系统管理结束wqg -->		                 		<!-- 交通法规wqg -->		<action name="ruleback_*" class="com.web.action.RuleAction" method="{1}">			<result name="default" type="dispatcher">rule/RuleViewList.jsp</result>			<result name="edit" type="dispatcher">rule/RuleEdit.jsp</result>			<result name="view" type="dispatcher">rule/RuleView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 宣教中心wqg -->		<action name="eduback_*" class="com.web.action.EduAction" method="{1}">			<result name="default" type="dispatcher">edu/EduViewList.jsp</result>			<result name="edit" type="dispatcher">edu/EduEdit.jsp</result>			<result name="view" type="dispatcher">edu/EduView.jsp</result>			<result name="video" type="dispatcher">edu/EduUpload2.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 驾驶员基本信息wqg -->		<action name="driverback_*" class="com.web.action.DriverAction" method="{1}">			<result name="default" type="dispatcher">driver/DriverViewList.jsp</result>			<result name="edit" type="dispatcher">driver/DriverEdit.jsp</result>			<result name="view" type="dispatcher">driver/DriverView.jsp</result>			<result name="caredit" type="dispatcher">driver/CarEdit.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 交警部门wqg -->		<action name="orgback_*" class="com.web.action.PoliceOrgAction" method="{1}">			<result name="default" type="dispatcher">org/PoliceOrgViewList.jsp</result>			<result name="edit" type="dispatcher">org/PoliceOrgEdit.jsp</result>			<result name="view" type="dispatcher">org/PoliceOrgView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 违法查询wqg -->		<action name="illegalback_*" class="com.web.action.IllegalAction" method="{1}">			<result name="default" type="dispatcher">illegal/IllegalViewList.jsp</result>			<result name="edit" type="dispatcher">illegal/IllegalEdit.jsp</result>			<result name="view" type="dispatcher">illegal/IllegalView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 快速处理点wqg -->		<action name="branchback_*" class="com.web.action.BranchAction" method="{1}">			<result name="default" type="dispatcher">branch/BranchViewList.jsp</result>			<result name="edit" type="dispatcher">branch/BranchEdit.jsp</result>			<result name="view" type="dispatcher">branch/BranchView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 安全提醒jjl -->		<action name="safenoticeback_*" class="com.web.action.SafeNoticeAction" method="{1}">			<result name="default" 	type="dispatcher">safenotice/safenotice_list.jsp</result>			<result name="edit"    	type="dispatcher">safenotice/safenotice_edit.jsp</result>			<result name="view"    	type="dispatcher">safenotice/safenotice_view.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action> 				<!-- 会议通知jjl -->		<action name="meetnoticeback_*" class="com.web.action.MeetNoticeAction" method="{1}">			<result name="default" 	type="dispatcher">meetnotice/meetnotice_list.jsp</result>			<result name="edit"    	type="dispatcher">meetnotice/meetnotice_edit.jsp</result>			<result name="view"    	type="dispatcher">meetnotice/meetnotice_view.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action> 				<!-- 问题互动ln -->		<action name="interchangeback_*" class="com.web.action.InterchangeAction" method="{1}">			<result name="default" type="dispatcher">interchange/InterchangeViewList.jsp</result>			<result name="edit" type="dispatcher">interchange/InterchangeEdit.jsp</result>			<result name="view" type="dispatcher">interchange/InterchangeView.jsp</result>			<result name="add" type="dispatcher">interchange/InterchangeAdd.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 微信微博wqg -->		<action name="micromessageback_*" class="com.web.action.MicromessageAction" method="{1}">			<result name="default" type="dispatcher">micromessage/MicromessageViewList.jsp</result>			<result name="edit" type="dispatcher">micromessage/MicromessageEdit.jsp</result>			<result name="view" type="dispatcher">micromessage/MicromessageView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 车辆管理jjl -->		<action name="carback_*" class="com.web.action.CarAction" method="{1}">			<result name="default" 	type="dispatcher">car/car_list.jsp</result>			<result name="edit"    	type="dispatcher">car/car_edit.jsp</result>			<result name="view"    	type="dispatcher">car/car_view.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>			<result name="addcar" 	type="dispatcher">pcar/PcarAdd.jsp</result>		</action>				<!-- 企业管理jjl -->		<action name="companyback_*" class="com.web.action.CompanyAction" method="{1}">			<result name="default" 	type="dispatcher">company/company_list.jsp</result>			<result name="companydefault" 	type="dispatcher">company/companylistforcompany.jsp</result>			<result name="edit"    	type="dispatcher">company/company_edit.jsp</result>			<result name="view"    	type="dispatcher">company/company_view.jsp</result>			<result name="safedefault" 	type="dispatcher">safegroup/viewAllSafeGroup.jsp</result>			<result name="safeedit"    	type="dispatcher">safegroup/editSafeGroup.jsp</result>			<result name="safeview"    	type="dispatcher">safegroup/viewOneSafeGroup.jsp</result>			<result name="safelistdefault" 	type="dispatcher">safegrouplist/viewAllSafeGroupList.jsp</result>			<result name="safelistedit"    	type="dispatcher">safegrouplist/editSafeGroupList.jsp</result>			<result name="safelistview"    	type="dispatcher">safegrouplist/viewOneSafeGroupList.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action> 					<!-- 安全组分组wqg -->		<action name="safegrouplistback_*" class="com.web.action.SafeGroupListAction" method="{1}">			<result name="default" 	type="dispatcher">safegrouplist/viewAllSafeGroupList.jsp</result>			<result name="edit"    	type="dispatcher">safegrouplist/editSafeGroupList.jsp</result>			<result name="view"    	type="dispatcher">safegrouplist/viewOneSafeGroupList.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action>					<!-- 安全组人员，车辆分配wqg -->		<action name="safegroupback_*" class="com.web.action.SafeGroupAction" method="{1}">			<result name="default" 	type="dispatcher">safegroup/viewAllSafeGroup.jsp</result>			<result name="edit"    	type="dispatcher">safegroup/editSafeGroup.jsp</result>			<result name="view"    	type="dispatcher">safegroup/viewOneSafeGroup.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action>					<!-- 安全组人员，车辆对应关系wqg -->		<action name="safegroupcardriverback_*" class="com.web.action.SafeGroupCarDriverAction" method="{1}">			<result name="default" 	type="dispatcher">safegroupcardriver/viewAllSafeGroupCarDriver.jsp</result>			<result name="edit"    	type="dispatcher">safegroupcardriver/editSafeGroupCarDriver.jsp</result>			<result name="view"    	type="dispatcher">safegroupcardriver/viewOneSafeGroupCarDriver.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action>								<!-- 交警基本信息管理ln -->		<action name="policeback_*" class="com.web.action.PoliceAction" method="{1}">			<result name="default" type="dispatcher">police/PoliceViewList.jsp</result>			<result name="edit" type="dispatcher">police/PoliceEdit.jsp</result>			<result name="view" type="dispatcher">police/PoliceView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 地域管理jjl -->		<action name="areaback_*" class="com.web.action.AreaAction" method="{1}">			<result name="default" 	type="dispatcher">area/area_list.jsp</result>			<result name="solist" 	type="dispatcher">area/area_sonlist.jsp</result>			<result name="edit" 	type="dispatcher">area/area_edit.jsp</result>			<result name="view" 	type="dispatcher">area/area_view.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action>				<!-- 机构管理jjl -->		<action name="policeorgback_*" class="com.web.action.OrgAction" method="{1}">			<result name="default" 	type="dispatcher">policeorg/policeorg_list.jsp</result>			<result name="solist" 	type="dispatcher">policeorg/policeorg_sonlist.jsp</result>			<result name="edit" 	type="dispatcher">policeorg/policeorg_edit.jsp</result>			<result name="view" 	type="dispatcher">policeorg/policeorg_view.jsp</result>			<result name="success" 	type="dispatcher">success.jsp</result>		</action>				<!-- 地区树查询 -->    	<action name="areatree" class="com.web.action.tree.TreeAction" method="areatree">    	</action>    	    	<!-- 机构树查询 -->    	<action name="orgtree" class="com.web.action.tree.TreeAction" method="orgtree">    	</action>    	    	<!-- 权限树查询 -->    	<action name="pertree" class="com.web.action.tree.TreeAction" method="pertree">    	</action>    			<!-- 交警基本信息管理（车辆划归）ln -->		<action name="pcarback_*" class="com.web.action.PcarAction" method="{1}">			<result name="default" type="dispatcher">pcar/PcarViewList.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>				<!-- 安全教育学习 -->		<action name="safelearnback_*" class="com.web.action.SafeLearnAction"			method="{1}">			<result name="default" type="dispatcher">safelearn/safelearn.jsp</result>			<result name="view" type="dispatcher">safelearn/safelearnview.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>					<!-- 数据管理员看到的通报批评 -->		<action name="reprimandback_*" class="com.web.action.ReprimandAction" method="{1}">			<result name="default" type="dispatcher">reprimand/ReprimandViewList.jsp</result>			<result name="edit" type="dispatcher">reprimand/ReprimandEdit.jsp</result>			<result name="view" type="dispatcher">reprimand/ReprimandView.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>						<!-- 企业组看到的通报批评 -->		<action name="otherreprimandback_*" class="com.web.action.OtherReprimandAction" method="{1}">			<result name="default" type="dispatcher">reprimand/OtherReprimandViewList.jsp</result>			<result name="viewother" type="dispatcher">reprimand/ViewOtherReprimandViewList.jsp</result>			<result name="edit" type="dispatcher">reprimand/OtherReprimandEdit.jsp</result>			<result name="editsubmod" type="dispatcher">reprimand/SubModReprimandEdit.jsp</result>			<result name="viewsubmod" type="dispatcher">reprimand/SubModReprimandView.jsp</result>			<result name="view" type="dispatcher">reprimand/OtherReprimandView.jsp</result>			<result name="viewdriver" type="dispatcher">reprimand/DriverReprimandView.jsp</result>			<result name="sub" type="dispatcher">reprimand/subMod.jsp</result>			<result name="detail" type="dispatcher">reprimand/reprimandDetail.jsp</result>			<result name="success" type="dispatcher">success.jsp</result>		</action>			</package>				<!-- 前台页面显示 -->	<package name="com.web.context" namespace="/" extends="struts-default">		<!-- 关于首页的action -->		<action name="index_*" class="com.web.action.front.IndexAction"			method="{1}">			<result name="view" type="dispatcher">/page/index.jsp</result>		</action>				<!-- 关于交通法规的action -->		<action name="rule_*" class="com.web.action.front.RuleAction"			method="{1}">			<result name="success" type="dispatcher">/page/rule/${page}</result>		</action>				<!-- 交通法规  ajax获取数据的action -->		<action name="getRuleDate*" class="com.web.action.front.RuleAction"			method="{1}">		</action>				<!-- 关于宣教中心的action -->		<action name="edu_*" class="com.web.action.front.EduAction"			method="{1}">			<result name="success" type="dispatcher">/page/edu/${page}</result>		</action>				<!-- 宣教中心 ajax获取数据的action -->		<action name="getEduDate*" class="com.web.action.front.EduAction"			method="{1}">		</action>				<!-- 关于基本信息的action -->		<action name="driver_*" class="com.web.action.front.DriverAction"			method="{1}">			<result name="success" type="dispatcher">/page/driver/${page}</result>		</action>				<!-- 基本信息 ajax获取数据的action -->		<action name="getDriverDate*" class="com.web.action.front.DriverAction"			method="{1}">		</action>				<!-- 关于违法查询的action -->		<action name="ruleIllegal_*" class="com.web.action.front.RuleIllegalAction"			method="{1}">			<result name="success" type="dispatcher">/page/ruleIllegal/${page}</result>		</action>				<!-- 违法查询 ajax获取数据的action -->		<action name="getRuleIllegalDate*" class="com.web.action.front.RuleIllegalAction"			method="{1}">		</action>				<!-- 关于快速处理点的action -->		<action name="branch_*" class="com.web.action.front.BranchAction"			method="{1}">			<result name="success" type="dispatcher">/page/branch/${page}</result>		</action>				<!-- 快速处理点 ajax获取数据的action -->		<action name="getBranchDate*" class="com.web.action.front.BranchAction"			method="{1}">		</action>				<!-- 关于问题互动的action -->		<action name="interchange_*" class="com.web.action.front.InterchangeAction"			method="{1}">			<result name="success" type="dispatcher">/page/interchange/${page}</result>		</action>				<!-- 基本信息 ajax获取数据的action -->		<action name="getInterchangeDate*" class="com.web.action.front.InterchangeAction"			method="{1}">		</action>						<!-- wqg -->		<!-- 关于安全提醒的action -->		<action name="safeNotice_*" class="com.web.action.front.SafeNoticeAction"			method="{1}">			<result name="success" type="dispatcher">/page/notice/${page}</result>		</action>				<!-- 安全提醒 ajax获取数据的action -->		<action name="getSafeNoticeData*" class="com.web.action.front.SafeNoticeAction"			method="{1}">		</action>				<!-- 关于会议通知的action -->		<action name="meetNotice_*" class="com.web.action.front.MeetNoticeAction"			method="{1}">			<result name="success" type="dispatcher">/page/notice/${page}</result>		</action>				<!-- 会议通知ajax获取数据的action -->		<action name="getMeetNoticeData*" class="com.web.action.front.MeetNoticeAction"			method="{1}">		</action>		<!-- 关于微信互动的action -->		<action name="micromessage_*" class="com.web.action.MicromessageAction"			method="{1}">			<result name="success" type="dispatcher">/page/microblog/${page}</result>		</action>				<!-- 基本信息 ajax获取数据的action -->		<action name="getMicromessageDate*" class="com.web.action.MicromessageAction"			method="{1}">		</action>				<!-- 关于车辆管理的action -->		<action name="car_*" class="com.web.action.front.CarAction"			method="{1}">			<result name="success" type="dispatcher">/page/car/${page}</result>		</action>				<!-- 车辆管理  ajax获取数据的action -->		<action name="getCarData*" class="com.web.action.front.CarAction"			method="{1}">		</action>				<!-- 关于企业管理的action -->		<action name="company_*" class="com.web.action.front.CompanyAction"			method="{1}">			<result name="success" type="dispatcher">/page/company/${page}</result>		</action>				<!-- 企业管理  ajax获取数据的action -->		<action name="getCompanyData*" class="com.web.action.front.CompanyAction"			method="{1}">		</action>			</package>

</struts>