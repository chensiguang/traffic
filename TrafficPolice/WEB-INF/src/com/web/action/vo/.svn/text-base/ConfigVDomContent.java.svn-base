package com.web.action.vo;


import com.web.pojo.Config;

/**
 * 网站配置对象VO
 * 
 * @author yin
 * 
 *         2013-4-1
 */
public class ConfigVDomContent
{

	private String showDataTime;
	private String siteName; // 站点名称
	private String keywords; // 网页关键字
	private String pdescription; // 网站描述
	private String address; // 公司地址
	private String code; // 邮编
	private String telephone; // 总机
	private String telephoneSale; // 销售电话
	private String telephoneService; // 客服电话
	private String fax; // 传真
	private String email; // 电子邮件
	private String homePage; // 主页
	private String qq1;
	private String qq2;
	private String msn1;
	private String msn2;

	/**
	 * 0 - 否 ; 1 - 是
	 */
	private int isimgcontent;

	public ConfigVDomContent()
	{

	}

	public ConfigVDomContent(Config config)
	{
		if (null != config)
		{

			this.siteName = config.getSiteName();
			this.keywords = config.getKeywords();
			this.address = config.getAddress();
			this.pdescription = config.getPdescription();
			this.code = config.getCode();
			this.telephone = config.getTelephone();
			this.telephoneSale = config.getTelephoneSale();
			this.telephoneService = config.getTelephoneService();
			this.fax = config.getFax();
			this.email = config.getEmail();
			this.homePage = config.getHomePage();
			this.qq1 = config.getQq1();
			this.qq2 = config.getQq2();
			this.msn1 = config.getMsn1();
			this.msn2 = config.getMsn2();

			// this.showDataTime = sdf.format(config.getAddDateTime());
		}
	}

	public String getShowDataTime()
	{
		return showDataTime;
	}

	public void setShowDataTime(String showDataTime)
	{
		this.showDataTime = showDataTime;
	}

	public int getIsimgcontent()
	{
		return isimgcontent;
	}

	public void setIsimgcontent(int isimgcontent)
	{
		this.isimgcontent = isimgcontent;
	}

	public String getSiteName()
	{
		return siteName;
	}

	public void setSiteName(String siteName)
	{
		this.siteName = siteName;
	}

	public String getKeywords()
	{
		return keywords;
	}

	public void setKeywords(String keywords)
	{
		this.keywords = keywords;
	}

	public String getPdescription()
	{
		return pdescription;
	}

	public void setPdescription(String pdescription)
	{
		this.pdescription = pdescription;
	}

	public String getAddress()
	{
		return address;
	}

	public void setAddress(String address)
	{
		this.address = address;
	}

	public String getCode()
	{
		return code;
	}

	public void setCode(String code)
	{
		this.code = code;
	}

	public String getTelephone()
	{
		return telephone;
	}

	public void setTelephone(String telephone)
	{
		this.telephone = telephone;
	}

	public String getTelephoneSale()
	{
		return telephoneSale;
	}

	public void setTelephoneSale(String telephoneSale)
	{
		this.telephoneSale = telephoneSale;
	}

	public String getTelephoneService()
	{
		return telephoneService;
	}

	public void setTelephoneService(String telephoneService)
	{
		this.telephoneService = telephoneService;
	}

	public String getFax()
	{
		return fax;
	}

	public void setFax(String fax)
	{
		this.fax = fax;
	}

	public String getEmail()
	{
		return email;
	}

	public void setEmail(String email)
	{
		this.email = email;
	}

	public String getHomePage()
	{
		return homePage;
	}

	public void setHomePage(String homePage)
	{
		this.homePage = homePage;
	}

	public String getQq1()
	{
		return qq1;
	}

	public void setQq1(String qq1)
	{
		this.qq1 = qq1;
	}

	public String getQq2()
	{
		return qq2;
	}

	public void setQq2(String qq2)
	{
		this.qq2 = qq2;
	}

	public String getMsn1()
	{
		return msn1;
	}

	public void setMsn1(String msn1)
	{
		this.msn1 = msn1;
	}

	public String getMsn2()
	{
		return msn2;
	}

	public void setMsn2(String msn2)
	{
		this.msn2 = msn2;
	}

}
