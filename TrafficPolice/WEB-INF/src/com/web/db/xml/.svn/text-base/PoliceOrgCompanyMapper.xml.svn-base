<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" ><mapper namespace="com.web.dao.PoliceOrgCompanyDao">	<!-- 交警部门 与公司-->	<resultMap id="BaseResultMap" type="com.web.pojo.PoliceOrgCompany">		<id 	column="pcid" 		property="pcid" 		jdbcType="INTEGER" />		<result column="orgid" 		property="orgid" 		jdbcType="INTEGER" />		<result column="comid" 		property="comid" 		jdbcType="INTEGER" />		<result column="operid"   	property="operid" 		jdbcType="INTEGER" />	    <result column="opertime" 	property="opertime" 	jdbcType="TIMESTAMP" />	    <result column="regid" 		property="regid" 		jdbcType="INTEGER" />	    <result column="regtime" 	property="regtime" 		jdbcType="TIMESTAMP" />	    <result column="state" 		property="state" 		jdbcType="INTEGER" />	    <result column="remark" 	property="remark" 		jdbcType="VARCHAR" />	 <association property="admin" javaType="com.web.pojo.Admin">        <id property="userno" jdbcType="INTEGER" />       <result property="username" jdbcType="VARCHAR" />    </association>	</resultMap>	  		<!-- 查找操作人 --><!-- 	<select id="selectUsernameById" resultMap="BaseResultMap"		parameterType="com.web.pojo.PoliceOrgCompany">		select i.*, u.username,u.userno from tp_basic_porg_company i left join tp_sys_user u        on i.operid = u.userno where i.pcid = #{id,jdbcType=INTEGER} 	</select> -->		<!-- 根据交警部门的id号查询管辖的货运企业 -->	 <select id="queryAllByDwid" resultMap="BaseResultMap" parameterType="java.lang.Integer">		select *  		from tp_basic_porg_company		where state = 1 		and orgid = #{dwid}  	</select>	  	<!-- 后台管理 -->  	  	<!-- 新增-提交 -->  	<insert id="insertPoliceOrgCompany" parameterType="com.web.pojo.PoliceOrgCompany">		insert into tp_basic_porg_company 		(			pcid,orgid,comid,operid,opertime,state,remark,regtime,regid		)		values		(			#{id,jdbcType=INTEGER},			#{orgid,jdbcType=INTEGER},			#{comid,jdbcType=INTEGER},	   		#{operid,jdbcType=VARCHAR},	   		#{opertime,jdbcType=TIMESTAMP},	   		#{state,jdbcType=INTEGER},	   		#{remark,jdbcType=VARCHAR},	   		#{regtime,jdbcType=TIMESTAMP},	   		#{regid,jdbcType=VARCHAR}		)  	</insert>  	  	<!-- 修改-提交 -->    <update id="updatePoliceOrgCompany" parameterType="com.web.pojo.PoliceOrgCompany">		update tp_basic_porg_company 		set 	   		orgid 		= #{orgid,jdbcType=INTEGER},	   		operid      = #{operid,jdbcType=VARCHAR},	   		opertime    = #{opertime,jdbcType=TIMESTAMP},	   		remark      = #{remark,jdbcType=VARCHAR} 		where state = 1    		and comid = #{comid,jdbcType=INTEGER}   	</update>  	</mapper>