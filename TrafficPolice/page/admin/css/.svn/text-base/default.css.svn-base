.menu {
	font-family:Verdana, Arial, Helvetica, sans-serif;
	width:150px;
	height:25px;
	margin-top:20px;
	padding:0px;
	font-size:12px; 
	margin:0px 0;
	text-align:left;
}
.menu ul li a, .menu ul li a:visited {
	display:block; 
	text-decoration:none; 
	width:140px; 
	height:25px; 
	text-align:center; 
	color:#444; 
	background:url(../images/menu_ul_bg.jpg);
	line-height:20px; 
	font-size:12px; 
	z-index:20;
	overflow:hidden;
}


.menu ul {padding:0; margin:0;list-style-type: none; }
.menu ul li {float:left; margin-left:4px; margin-right:1px; position:relative;margin-top:3px;;
border:#999999 double 1px;
height:25px;
}
.menu ul li ul {display: none;z-index:20;}
/* specific to non IE browsers */
.menu ul li:hover a {
	background:#CCCCCC;
}
.menu ul li:hover a {
	color:#000; 
	background:#CCCCCC;
	border-left:1px solid #CCCCCC;
	border-top:1px solid #cccccc;
	border-right:1px solid #cccccc;
	border-bottom:1px solid #cccccc;
}
.menu ul li:hover ul {display:block; position:absolute; top:-4px; left:143px; width:145px; background:#FFFFFF}
.menu ul li:hover ul li a.hide {background:#CCCCCC; color:#fff;}
.menu ul li:hover ul li:hover a.hide {background:#CCCCCC; color:#444;}
.menu ul li:hover ul li ul {display: none;}
.menu ul li:hover ul li a {display:block; background:url(../images/menu_ul_bg.jpg); color:#444;border-bottom:1px solid #999999;}
.menu ul li:hover ul li a:hover {
	background:#CCCCCC; 
	color:#000;
}
.menu ul li:hover ul li:hover ul {
	display:block; 
	position:absolute; 
	left:143px;
	top:-4px;;
}


