/*@import "menu.css";*/
body, h1, h2, h3, h4, h5, h6, hr, p,
blockquote, dl, dt, dd, ul, ol, li,
pre,form, fieldset, legend, button, input, textarea,
th, td, img{
margin: 0;
padding: 0;
}
body,button, input, select, textarea {
font: 12px/1.5 '宋体',tahoma, Srial, helvetica, sans-serif; }
h1, h2, h3, h4, h5, h6 { font-size: 100%; }
em{font-style:normal;}
ul, ol { list-style: none; }
a { text-decoration: none; color:#000;}
a:hover { text-decoration: underline; color:#3363a7; }
img{ border:0px;}
table { border-collapse: collapse; border-spacing: 0; }

.clear{
    height:0px;
    line-height:0px;
    font-size:0px;
    clear:both;
    }
.upload * { float:left; margin-right:5px;}
.fleft{ float:left}
.fright{ float:right}
#header{ height:100px; background:url(../images/repeat-x.png) repeat-x;}
.logo{ width:511px; height:100px; background:url(/TrafficPolice/page/admin/images/logo.png);background-repeat:no-repeat;margin-left:25px;}
.logoright{ width:251px; height:100px; background:url(/TrafficPolice/page/admin/images/top_img.png);background-repeat:no-repeat;}
.logofont{font-size:12px; font-family:"Courier New", Courier, monospace; color:#FFFFF0; font-weight:600}
.logout{ width:56px; height:81px; background:url(../images/index-logout.png); margin-top:0px;}
.ztlr { padding:10px; line-height:200%;}
.nav-left{ width:34px; height:51px; background:url(../images/index-nav-left.png);}
.nav-right{ width:34px; height:51px; background:url(../images/index-nav-right.png);}
.subnav{ height:27px; background:url(../images/index-subnav-bg.gif)}
.subnav .subnavLeft{ width:17px; height:27px; background:url(../images/index-subnav-left.gif)}
.subnav .subnavRight{ width:17px; height:27px; background:url(../images/index-subnav-right.gif)}
.menu{ width:168px; height:555px;background:#e6f4fa; color:#075587; margin-left:5px;}
.menu ul{ width:168px;}
.menu ul li{ width:168px; background:url(../images/index-menu-bg.gif) no-repeat; text-align:center}
.menu ul li a{ height:26px; line-height:26px; color:#075587}
.menu ul li a:hover{ height:26px; line-height:26px; color:#075587; text-decoration:none; margin-left:10px;}
.menu .subMenuTitle{ background:url(../images/index-menu-title.gif); text-align:center; height:27px; line-height:26px; color:#fff}
.menu ul li ul{ width:168px;}
.menu ul li ul li{ background:url(../images/index-submenu-bg.gif);}



.sidebar{ width:10px; height:100%;}
.sidebar .btn{ width:5px; height:39px; background:url(../images/sidebar-on.gif); margin-top:200px;}
.sidebar .btn:hover{ background-position:0 -39px;}
iframe{ margin:0 auto;}
.space{ height:5px; font-size:0px; background:#198bc9}
#footer{ width:100%; height:26px; line-height:26px; background:url(../images/index-footer-bg.gif);}


.pageTitle{padding-left:20px; height:27px; line-height:27px; color:#075587; background:url(../images/page-pageTitle-bg.gif);}
.pageTitle span{ margin-left:10px;}
.pageButton{ height:30px;}


.pageColumn{ background:url(../images/page-bg.gif) repeat-x; padding:5px;}
.pageColumn table{ width:100%;margin-bottom:5px;}
.pageColumn thead{ border:1px solid #bfd8e0; color:#075587; background:url(../images/page-thead-bg.gif);}
.pageColumn thead th{ border-left:1px solid #c5dbe2; text-align:center; height:28px;}
.pageColumn tbody tr{ border:1px solid #bfd8e0; height:28px;}
.pageColumn tbody .trLight{ background:#f9fcfd}
.pageColumn tbody .checkBox{ background:#f1f7f9; border:1px solid #bfd8e0; text-align:center}
.pageColumn tbody td{ text-align:center;}
/* .pageColumn */

.trstyle{ border:1px solid #bfd8e0; color:#075587; background:url(../images/page-thead-bg.gif);}
.trstyle1{ border:0px solid #bfd8e0; color:#075587; background:url(../images/page-thead-bg.gif);}

/*分页样式*/
.page{ text-align:right; clear:both; margin-right:10px}
.page a{ border:1px solid #64a1da; padding:2px 6px; margin:0px 2px; color:#2d64b3;}
.page a:hover{ background-color: #2d64b3; color: #fff; border: 1px solid #255296; }
.page b{ background-color: #2d64b3; color: #fff; border: 1px solid #255296; padding:2px 6px; margin:0px 2px;}
.page .close{ border:1px solid #ccc; padding:2px 6px; margin:0px 2px; color:#ccc;}

/*content */
.lright{ float: right; width: 600px; padding: 0px 10px; }
.leirong h1{ text-align:center; font-size:14px; padding:20px 0px; color:#333;}
.leirong h2{ color:#666; background-color:#ddecf4; font-size:12px; padding:6px 0px; text-align:center; font-weight:normal; margin-bottom:10px;}
.leirong h2 i{ float:right; margin-right:10px; font-style:normal;}
.leirong .ztlr { padding:10px; line-height:200%;}
.box3 { background-color: #fff; }
.box3 .boxb,.box3 .boxt, .box3 .boxb .l, .box3 .boxb .r,.box3 .boxt .l, .box3 .boxt .r { height: 10px; overflow: hidden; font-size: 10px; line-height: 10px; }
.box3 .boxb .l, .box3 .boxb .r,.box3 .boxt .l, .box3 .boxt .r { background-image: url(../images/box3_rou.png); background-repeat: no-repeat; width: 10px; }
.box3 .boxb .l { float: left; background-position: left bottom; }
.box3 .boxb .r { float: right; background-position: right bottom; }
.box3 .boxt .l { float: left; background-position: left top; }
.box3 .boxt .r { float: right; background-position: right top; }
.box3 .boxb { background: url(../images/box3_top.png) repeat-x left bottom; }
.box3 .boxt { background: url(../images/box3_top.png) repeat-x left top; }
.box3 .boxc { background: url(../images/box3_left.png) repeat-y left top; }
.box3 .boxcr { background: url(../images/box3_left.png) repeat-y right top; padding: 0px 10px; }
em{color:#FF0000}

